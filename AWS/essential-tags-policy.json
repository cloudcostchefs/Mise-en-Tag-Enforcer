{
  "tags": {
    "Environment": {
      "tag_key": {
        "@@assign": "Environment"
      },
      "tag_value": {
        "@@assign": [
          "Production",
          "Development",
          "Test", 
          "Staging"
        ]
      },
      "enforced_for": {
        "@@assign": [
          "ec2:instance",
          "ec2:volume",
          "ec2:security-group",
          "ec2:vpc",
          "ec2:subnet",
          "s3:bucket",
          "rds:db",
          "rds:cluster",
          "lambda:function",
          "ecs:service",
          "ecs:cluster",
          "eks:cluster",
          "elasticloadbalancing:loadbalancer",
          "elasticloadbalancing:targetgroup"
        ]
      }
    },
    "Owner": {
